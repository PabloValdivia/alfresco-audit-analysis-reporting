      <link label-id="AAAR_Menu.free-analysis.four"  icon-class="" target-url="http://localhost:8080/pentaho/api/repos/%3Apublic%3AAAAR%3AOlap%3ARepository_Pivot4jFreeAnalysis.pivot4j/generatedContent" />
    </free-analysis>

    <!-- Link set of the credits. -->
    <credits>
      <link label-id="AAAR_Menu.credits" icon-class="" target-url="http://fcorti.com" />
    </credits>

