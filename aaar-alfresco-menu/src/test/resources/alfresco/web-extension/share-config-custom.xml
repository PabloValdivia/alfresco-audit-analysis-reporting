<alfresco-config>

  <!-- A.A.A.R. config section -->
  <config evaluator="string-compare" condition="AAAR" replace="true">

    <!-- Set to true or false to view the menu and/or credits. -->
    <visible>true</visible>
    <visible-credits>true</visible-credits>

    <!-- Link set of the dashboards. -->
    <dashboards>
      <link label-id="AAAR_Menu.dashboard.main"       icon-class="" target-url="http://localhost:8080/pentaho/api/repos/%3Apublic%3AAAAR%3Amain.wcdf/generatedContent" />
      <link label-id="AAAR_Menu.dashboard.audit"      icon-class="" target-url="http://localhost:8080/pentaho/api/repos/%3Apublic%3AAAAR%3Aaudit_actions.wcdf/generatedContent" />
      <link label-id="AAAR_Menu.dashboard.repository" icon-class="" target-url="http://localhost:8080/pentaho/api/repos/%3Apublic%3AAAAR%3Arepository.wcdf/generatedContent" />
      <link label-id="AAAR_Menu.dashboard.documents"  icon-class="" target-url="http://localhost:8080/pentaho/api/repos/%3Apublic%3AAAAR%3Arepository_documents.wcdf/generatedContent" />
      <link label-id="AAAR_Menu.dashboard.folders"    icon-class="" target-url="http://localhost:8080/pentaho/api/repos/%3Apublic%3AAAAR%3Arepository_folders.wcdf/generatedContent" />
    </dashboards>

    <!-- Link set of the reports. -->
    <reports>
      <link label-id="AAAR_Menu.report.one"       icon-class="" target-url="http://localhost:8080/pentaho/api/repos/:public:AAAR:Reports:audit_topTen.prpt/viewer" />
      <link label-id="AAAR_Menu.report.two"       icon-class="" target-url="http://localhost:8080/pentaho/api/repos/:public:AAAR:Reports:document_hours_of_activity.prpt/viewer" />
