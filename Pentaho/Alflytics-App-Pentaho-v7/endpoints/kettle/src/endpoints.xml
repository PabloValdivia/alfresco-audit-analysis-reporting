<endpoint id="alfrescoSearch">/alfresco/api/-default-/public/search/versions/1/search</endpoint>
<endpoint id="alfrescoSearchCategoriesBody">{"scope":{"stores":["workspace://SpacesStore"]},"query":{"query":"(TYPE:\"cm:category\")","language": "afts"},"sort":[{"type":"FIELD","field":"cm:modified","ascending": true},{"type":"FIELD","field":"sys:dbid","ascending":true}],"include":["properties"],"paging":{"maxItems": ${maxItems},"skipCount":${skipCount}}}</endpoint>
<endpoint id="alfrescoSearchCategoriesCountBody">{"scope":{"stores":["workspace://SpacesStore"]},"query":{"query": "(TYPE:\"cm:category\")","language":"afts"},"paging":{"maxItems":1,"skipCount":1000000000}}</endpoint>
<endpoint id="alfrescoSearchNodesModifiedBody">{"scope":{"stores":["workspace://SpacesStore"]},"query":{"query":"(((TYPE:\"cm:content\") OR (TYPE:\"cm:folder\")) AND NOT (TYPE:\"cm:systemfolder\") AND (@cm:modified:[\"${alfrescoMaxDt}\" TO MAX]))","language": "afts"},"sort":[{"type":"FIELD","field":"cm:modified","ascending": true},{"type":"FIELD","field":"sys:dbid","ascending":true}],"include":["properties","isLink"],"paging":{"maxItems": ${maxItems},"skipCount":${skipCount}}}</endpoint>
<endpoint id="alfrescoSearchNodesNotModifiedBody">{"scope":{"stores":["workspace://SpacesStore"]},"query":{"query":"(((TYPE:\"cm:content\") OR (TYPE:\"cm:folder\")) AND NOT (TYPE:\"cm:systemfolder\") AND (@cm:modified:[\"2001-01-01T00:00:00.000Z\" TO \"${alfrescoMaxDt}\"]))","language": "afts"},"sort":[{"type":"FIELD","field":"cm:modified","ascending": true},{"type":"FIELD","field":"sys:dbid","ascending":true}],"include":[],"paging":{"maxItems": ${maxItems},"skipCount":${skipCount}}}</endpoint>
<endpoint id="alfrescoSearchNodesCountBody">{"scope":{"stores":["workspace://SpacesStore"]},"query":{"query": "(((TYPE:\"cm:content\") OR (TYPE:\"cm:folder\")) AND NOT (TYPE:\"cm:systemfolder\") AND (@cm:modified:[\"2001-01-01T00:00:00.000Z\" TO MAX]))","language":"afts"},"paging":{"maxItems":1,"skipCount":1000000000}}</endpoint>
</endpoints>
