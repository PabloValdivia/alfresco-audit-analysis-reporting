     },
     {
      "name": "postExecution",
      "value": "function f() {\n\n    $('#databaseTypeField').children(\":first\").attr('class','form-control');\n\n    var databaseType = $.trim(Dashboards.getParameterValue('${p:databaseType}'));\n    Dashboards.setParameter('${p:databaseType}',databaseType);\n\n    if (databaseType != 'PostgreSql' && databaseType != 'MySql') {\n\n        $('#alertPanel').append('<div class=\"alert alert-warning alert-dismissible\" role=\"alert\" style=\"margin-left: 20px; margin-right: 20px;\"><button type=\"button\" class=\"close\" data-dismiss=\"alert\" aria-label=\"Close\"><span aria-hidden=\"true\">&times;</span></button><strong>Database type!</strong> Admitted values are \\'PostgreSql\\' or \\'MySql\\'.</div>');\n\n        databaseType = 'PostgreSql';\n        Dashboards.setParameter('${p:databaseType}',databaseType);\n        $('#render_databaseTypeComponent').val(databaseType);\n    }\n} ",
