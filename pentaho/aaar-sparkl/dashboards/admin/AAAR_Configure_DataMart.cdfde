     {
      "name": "preChange",
      "value": "",
      "type": "JavaScript"
     },
     {
      "name": "postChange",
      "value": "function f() {\n\n    if (Dashboards.getParameterValue('${p:databaseType}') == 'PostgreSql') {\n        Dashboards.fireChange('${p:databasePort}','5432');\n        Dashboards.fireChange('${p:databaseLogin}','postgres');\n        Dashboards.fireChange('${p:databasePassword}','postgres');\n        $('#databaseBinGroup').hide();\n    }\n    else if (Dashboards.getParameterValue('${p:databaseType}') == 'MySql') {\n        Dashboards.fireChange('${p:databasePort}','3306');\n        Dashboards.fireChange('${p:databaseLogin}','root');\n        Dashboards.fireChange('${p:databasePassword}','root');\n        $('#databaseBinGroup').show();\n    }\n\n    Dashboards.setParameter('${p:isChanged}','true');\n} ",
      "type": "JavaScript"
     },
