     {
      "name": "query",
      "value": "select\n d.id as date_id,\n coalesce(sum(a.num),0) as actions\nfrom\n dm_fact_actions a right join dm_dim_dates d on a.date_id = d.id and a.alfresco_id = ${alfrescoInstanceParam}\nwhere\n d.id between str_str_to_date(${dtFromParam},'%Y-%m-%d') and str_str_to_date(${dtToParam},'%Y-%m-%d')\ngroup by\n d.id\norder by\n d.id asc",
      "type": "SqlQuery"
     },
     {
