    "properties": [
     {
      "name": "name",
      "value": "documentsPerDay",
      "type": "Id"
     },
     {
      "name": "access",
      "value": "public",
      "type": "Access"
     },
     {
      "name": "jndi",
      "value": "AAAR_DataMart",
      "type": "Jndi"
     },
     {
      "name": "query",
      "value": "select\n dat.id as date_id,\n (select coalesce(count(*),0) from dm_dim_documents doc where doc.alfresco_id = ${alfrescoInstanceParam} and doc.creation_date_id <= dat.id) as Number,\n (select coalesce(sum(doc.size)/(1024*1024),0) from dm_dim_documents doc where doc.alfresco_id = ${alfrescoInstanceParam} and doc.creation_date_id <= dat.id) as Mb\nfrom\n dm_dim_dates dat\nwhere\n dat.id between str_to_date(${dtFromParam},'%Y-%m-%d') and str_to_date(${dtToParam},'%Y-%m-%d')\ngroup by\n dat.id\norder by\n dat.id asc",
      "type": "SqlQuery"
     },
     {
      "name": "parameters",
      "value": "[[\"alfrescoInstanceParam\",\"1\",\"Integer\",\"\"],[\"dtFromParam\",\"2015-01-01\",\"String\",\"\"],[\"dtToParam\",\"2015-01-01\",\"String\",\"\"]]",
