      "name": "parameters",
      "value": "[[\"alfrescoInstanceParam\",\"1\",\"Integer\",\"\"],[\"dtFromParam\",\"2015-01-01\",\"String\",\"\"],[\"dtToParam\",\"2015-01-01\",\"String\",\"\"],[\"workflowDefinitionIdParam\",\"-1\",\"Integer\",\"\"]]",
