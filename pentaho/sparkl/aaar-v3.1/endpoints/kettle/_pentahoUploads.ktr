    <definitions>
        <definition>
        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[
import java.io.InputStream;
import java.io.FileInputStream;
import java.io.FileNotFoundException;
import java.text.ParseException;

import com.sun.jersey.core.header.FormDataContentDisposition;
import org.apache.commons.lang.StringUtils;
import org.apache.commons.io.FilenameUtils;

import org.pentaho.platform.web.http.api.resources.RepositoryImportResource;
import org.pentaho.platform.api.repository2.unified.IUnifiedRepository;
import org.pentaho.platform.engine.core.system.PentahoSystem;
import org.pentaho.platform.api.repository2.unified.RepositoryFile;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException { 

	// First, get a row from the default input hop
	Object[] r = getRow();

    // If the row object is null, we are done processing.
	if (r == null) {
		setOutputDone();
		return false;
	}

	IUnifiedRepository repository = (IUnifiedRepository)PentahoSystem.get(IUnifiedRepository.class);
	if (repository == null) {
		logBasic("repository not created");
		return true;
	} else {
		logBasic("repository created");
	}

	// Creating target folder.
	String pentahoPath = StringUtils.chomp(getParameter("pentahoPath"),getParameter("file.separator"));
	logBasic("Creating target folder: " + pentahoPath);
	String folderName = FilenameUtils.getBaseName(pentahoPath);
	String folderPath = pentahoPath.substring(0, StringUtils.lastIndexOf(pentahoPath, folderName));
	if (repository.getFile(pentahoPath) == null) {
		RepositoryFile parentFolder = repository.getFile(folderPath);
		repository.createFolder(parentFolder.getId(), new RepositoryFile.Builder(folderName).folder(true).build() , "");
	} else
		logBasic("Folder '" + pentahoPath + "' already existed");

	// Variables
	String fileName = get(Fields.In, "fileName").getString(r); // getParameter("fileName");
	String fileSystemPath = getParameter("fileSystemPath").replace("file://","") + getParameter("file.separator") + fileName;
	InputStream input = null;
	try {
		input = new FileInputStream(fileSystemPath);
	} catch(FileNotFoundException ex) {
		logBasic("Inputstream: " + ex.getMessage());
	}

	FormDataContentDisposition fileInfo = null;
	try {
		fileInfo = new FormDataContentDisposition("form-data; name=\"" + fileName + "\"; filename=\"" + fileName + "\"");
	} catch(ParseException ex){
		logBasic("fileInfo: " + ex.getMessage());
	}

	logBasic("Ready to start importing '" + fileName + "' in '" + pentahoPath + "'");
	RepositoryImportResource repositoryImporter = new RepositoryImportResource();
	repositoryImporter.doPostImport(pentahoPath, input, "true", "true", "true", "true", "UTF-8", "DEBUG", fileInfo, null);
	logBasic("Finished importing '" + fileName + "'");

	return true;
}]]></class_source>
